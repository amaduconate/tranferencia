// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL_POSTGRES_URL")
}



model currency {  
  id Int @id @default(autoincrement())

  rubles Float
}

model commission {
  id Int @id @default(autoincrement())
  
  fromEur Float
  fromRub Float
  fromXof Float
}

model commissionPartnership {
  id Int @id @default(autoincrement())
  
  fromEurPartnership Float
  fromRubPartnership Float
  fromXofPartnership Float
}

model exchange_rate {
  id Int @id @default(autoincrement())

  fromEur Float
  fromRub Float
  fromXof Float
}